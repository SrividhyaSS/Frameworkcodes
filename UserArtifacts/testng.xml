<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
    
    <listeners>
        
        <listener class-name = "framestest.ListenerinTestNG" />                   
          
    </listeners>
    
  <test name="Test in Chrome">
      
      <parameter name = "browser" value = "chrome" />
      
    <classes>
      <class name="runs.LoginTest"/>
      <class name="runs.VerifyPageTitle"/>
      
    </classes>
    
    
  </test> <!-- Test -->
  
  <test name="Test in IE">
      
      <parameter name = "browser" value = "ie" />
      
    <classes>
      <class name="runs.LoginTest"/>
      <class name="runs.VerifyPageTitle"/>
    </classes>
    
    
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
